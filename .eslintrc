{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "env": {
    "es6": true,
    "browser": true
  },
  "plugins": ["prettier", "react", "react-hooks"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "consistent-return": 0
  }
}
